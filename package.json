{
	"name": "@downpourdigital/boxes-react-bridge",
	"version": "0.0.1",
	"description": "Renderer agnostic react bindings for boxes",
	"sideEffects": false,
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"repository": "https://github.com/johh/boxes-react-bridge.git",
	"author": "johh <code@johh.net>",
	"license": "BSD-4-Clause",
	"private": false,
	"scripts": {
		"build": "rollup -c",
		"declaration": "tsc --emitDeclarationOnly",
		"type-check": "tsc --noEmit",
		"dev": "rollup -c -w",
		"lint": "eslint --ext .ts,.tsx,.js src/"
	},
	"devDependencies": {
		"@downpourdigital/boxes": "^0.3.10",
		"@downpourdigital/scheduler": "^0.0.10",
		"@types/react": "^16.9.35",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-typescript": "^7.2.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^2.5.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.9",
		"react": "^16.13.1",
		"rollup": "^2.3.2",
		"rollup-plugin-typescript2": "^0.27.0",
		"typescript": "^3.8.3"
	},
	"peerDependencies": {
		"@downpourdigital/boxes": ">=0.3.10",
		"@downpourdigital/scheduler": ">=0.0.10",
		"react": ">=16"
	},
	"dependencies": {},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js}": "eslint --max-warnings 1"
	}
}
